 115  parameter
 116  *Supply risk interior factor (HHI and WGI) parameter definition
 117  riskfactor(i,j) 'Herfindahl-Hirschman Index for the producing countries * 
      World Governance Index for the producing countries'
 118   
 119  *Price Parameter definition
 120  averageprice(i) 'average price of the different minerals in the last 5 yea
      rs (2013-2018)'
 121  sumprice(i) 'price sum of the different minerals in the last 5 years (2013
      -2018)'
 122   
 123  *Production Parameter definition
 124  globalprod2013(i)'Global Primary Production of the minerales in 2013'
 125  globalprod2018(i) 'Global Primary Production of the minerales in 2015';
 126   
 127  *Supply risk interior factor (HHI and WGI) parameter definition
 128  riskfactor("Titanium",j) = srti (j,"S2")* srti (j,"WGITotal");
 129  riskfactor("Vanadium",j) = srva (j,"S2")* srva (j,"WGITotal");
 130  riskfactor("Chromium",j) = srcr (j,"S2")* srcr (j,"WGITotal");
 131  riskfactor("Antimony",j) = srsb (j,"S2")* srsb (j,"WGITotal");
 132  riskfactor("Beryllium",j) = srbe (j,"S2")* srbe (j,"WGITotal");
 133  riskfactor("Niobium",j) = srnb (j,"S2")* srnb (j,"WGITotal");
 134  riskfactor("Lithium",j) = srli (j,"S2")* srli (j,"WGITotal");
 135  riskfactor("Zirconium",j) = srzr (j,"S2")* srzr (j,"WGITotal");
 136   
 137  *Price parameter Equation definition (row)
 138  averageprice(i) = (md (i,"Price2013")+ md (i,"Price2014") + md (i,"Price20
      15") + md (i,"Price2016") + md (i,"Price2017") + md (i,"Price2018"))/5;
 139  sumprice(i) = md (i,"Price2013")+ md (i,"Price2014") + md (i,"Price2015") 
      + md (i,"Price2016") + md (i,"Price2017") + md (i,"Price2018");
 140   
 141  *Production parameter Equation definition (row)
 142  globalprod2013(i) = pg (i, "Production2013");
 143  globalprod2018(i) = pg (i, "Production2018");
 144   
